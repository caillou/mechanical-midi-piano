{
    "name": "SolenoidDriver",
    "version": "1.0.0",
    "description": "High-level, safety-aware driver for Adafruit I2C Solenoid Driver boards (MCP23017-based). Designed for the Mechanical MIDI Piano project.",
    "keywords": [
        "solenoid",
        "mcp23017",
        "i2c",
        "gpio",
        "midi",
        "piano",
        "teensy"
    ],
    "authors": [
        {
            "name": "Mechanical MIDI Piano Project",
            "maintainer": true
        }
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/mechanical-midi-piano/mechanical-midi-piano.git"
    },
    "license": "MIT",
    "frameworks": "arduino",
    "platforms": [
        "teensy",
        "espressif32",
        "atmelavr",
        "atmelsam"
    ],
    "dependencies": {
        "adafruit/Adafruit MCP23017 Arduino Library": "^2.3.0",
        "adafruit/Adafruit BusIO": "^1.14.0"
    },
    "export": {
        "include": [
            "SolenoidConfig.h",
            "SolenoidChannel.h",
            "SolenoidChannel.cpp",
            "SolenoidDriver.h",
            "SolenoidDriver.cpp",
            "library.json"
        ]
    }
}
